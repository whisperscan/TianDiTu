<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <com.tianditu.android.maps.MapView 
		android:id="@+id/driving_mapview"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"/>
	<LinearLayout 
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:orientation="vertical">
		<LinearLayout 
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			android:orientation="horizontal">
			<Button
				android:id="@+id/start"
				android:text="起点"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/one"
				android:text="点1"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/two"
				android:text="点2"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/three"
				android:text="点3"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/four"
				android:text="点4"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/end"
				android:text="终点"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/route"
				android:text="规划"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
		</LinearLayout>
		<LinearLayout 
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			android:orientation="horizontal">
			<Button
				android:id="@+id/fastest"
				android:text="最快"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/shortest"
				android:text="最短"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
			<Button
				android:id="@+id/not_highway"
				android:text="不走高速"
				android:textSize="12sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</Button>
		</LinearLayout>
	</LinearLayout>
</FrameLayout>